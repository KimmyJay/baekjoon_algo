   "source": [
    "# small = 3 big = 5\n",
    "# return min amount of pouches needed for goal or -1 if goal unattainable\n",
    "\n",
    "# \n",
    "goal = int(input())\n",
    "count = 0\n",
    "while goal >= 0:\n",
    "    if goal % 5 == 0:\n",
    "        count += (goal // 5)\n",
    "        print(count)\n",
    "        break\n",
    "    goal -= 3\n",
    "    count += 1\n",
    "else:\n",
    "    print(-1)"
   ]
