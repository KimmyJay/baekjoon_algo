    "def narcissistic( value ):\n",
    "    digits = list(map(int, str(value)))\n",
    "    sum = 0\n",
    "    for digit in digits:\n",
    "        sum += digit ** len(digits)\n",
    "    \n",
    "    return int(''.join(str(n) for n in digits)) == sum"
   ]
  }
