    "def xbonacci(signature, n):\n",
    "    x = len(signature)\n",
    "    res = signature[:n]\n",
    "    \n",
    "    for i in range(x, n):\n",
    "        res.append(sum(res[-x:]))\n",
    "        \n",
    "    return res"
