    "def tribonacci(signature, n):\n",
    "    if n == 0:\n",
    "        return []\n",
    "    if n < 3:\n",
    "        return [signature[i] for i in range(0,n)]\n",
    "    for i in range(3,n):\n",
    "        signature.append(signature[i-1] + signature[i-2] + signature[i-3])\n",
    "    return signature\n",
    "\n",
    "print(tribonacci([2, 4, 9], 13))\n"
   ]
  }
