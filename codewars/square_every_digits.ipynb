   "source": [
    "# square every digit of a given number and concatenate them\n",
    "\n",
    "def square_digits(num):\n",
    "    result = ''\n",
    "    for digit in str(num):\n",
    "        result += str(int(digit)**2)\n",
    "    \n",
    "    return int(result)\n",
    "    "
   ]
